{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Generated schema for Root",
  "type": "object",
  "properties": {
    "uas": {
      "type": "object",
      "properties": {
        "model": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "weigth": {
          "type": "number"
        },
        "maxSpeed": {
          "type": "number"
        },
        "heightOfFlightGeography": {
          "type": "number"
        },
        "maxAutonomy": {
          "type": "number"
        },
        "characteristicDimension": {
          "type": "number"
        }
      },
      "required": [
        "model",
        "type",
        "weigth",
        "maxSpeed",
        "heightOfFlightGeography",
        "maxAutonomy",
        "characteristicDimension"
      ]
    },
    "altimetry": {
      "type": "number"
    },
    "operationType": {
      "type": "string",
      "enum": ["BVLOS", "VLOS", "EVLOS"]
    },
    "maxWindSpeeed": {
      "type": "number"
    },
    "maxPitch": {
      "type": "number"
    },
    "maxSail": {
      "type": "number"
    },
    "containment": {
      "type": "string",
      "enum": ["low", "medium", "high"]
    }
  },
  "required": ["uas", "altimetry", "operationType", "maxWindSpeeed", "maxPitch"]
}
